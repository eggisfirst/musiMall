<view class='orderDetails-container'>
  <compTab 
          class='tab'
          catch:getCurrentTab="getCurrentTab" 
          current="{{current}}" 
          list="{{list}}"/>
  <compContent 
              class='content'
              hasMoreData="{{hasMoreData}}"
              noData="{{noData}}"
              current="{{current}}"
              contenList="{{contenList}}"
              type="order"
              key="{{key}}"/>
</view>
